//Realizar un algoritmo que me permita pasar de horas a minutos y segundos
Algoritmo horasAMinutosYSegundos
	Definir H Como Real;
	Definir M Como Real;
	Definir S Como Real;
	Escribir "Introduce el valor de H: ";
	Leer H;
	M<-H*60/1;
	S<-M*60/1;
	Escribir "El resultado de ",H;
	Escribir "horas en minutos es: ",M;
	Escribir "El resultado de ",H;
	Escribir "horas en segundos es: ",S;
FinAlgoritmo
