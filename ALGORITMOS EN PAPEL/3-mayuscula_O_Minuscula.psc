// Desarrollar un algoritmo que me capture un texto y lo muestre en mayúsculas y minúsculas

Algoritmo mayuscula_O_Minuscula
	Definir tex Como Caracter;
	Escribir Sin Saltar "Introduzca un texto: ";
	Leer tex;
	Escribir "";
	Escribir Mayusculas(tex);
	Escribir Minusculas(tex);
	
FinAlgoritmo
